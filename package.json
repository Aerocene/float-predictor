    "lint": "eslint --ext .js,.vue src test/unit",
    "build": "node build/build.js",
    "deploy-staging": "DEPLOY_HOSTNAME=eu-west-1.galaxy-deploy.meteor.com meteor deploy https://staging.aerocene.org --settings settings/staging.json --deploy-polling-timeout 2400000",
    "deploy-production": "DEPLOY_HOSTNAME=eu-west-1.galaxy-deploy.meteor.com meteor deploy https://floatpredictor.aerocene.org --settings settings/production.json --deploy-polling-timeout 2400000"
