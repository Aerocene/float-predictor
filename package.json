{
  "name": "aerocene",
  "version": "1.0.0",
  "description": "Aerocene Web Application. Aerocene is a multi-disciplinary project that foregrounds the artistic and scientific exploration of environmental issues.",
  "author": "Studio Folder",
  "private": true,
  "scripts": {
    "start": "meteor run --settings settings/local.json",
